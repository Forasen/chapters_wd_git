#include<iostream>

class point1
{
private:
    int m_ix;
    int m_iy;
public:
    point1(int val):m_iy(val), m_ix(m_iy)
    {
        std::cout<<"m_ix="<<m_ix<<"m_iy="<<m_iy<<std::endl;
    }
};

int main(void)
{
    point1 pt1(3);
    pt1.point1();

    return 0;
}














}
